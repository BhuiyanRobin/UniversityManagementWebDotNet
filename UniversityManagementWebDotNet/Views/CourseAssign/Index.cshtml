@model IEnumerable<UniversityManagementWebDotNet.Models.DAO.CourseAssign>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ACourse.CourseName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AFaculty.FacultyName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ARoom.RoomName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ASemister.SemistereName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ATimeSlot.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AWeekDaysS.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Section)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ACourse.CourseName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AFaculty.FacultyName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ARoom.RoomName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ASemister.SemistereName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ATimeSlot.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AWeekDaysS.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Section)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
